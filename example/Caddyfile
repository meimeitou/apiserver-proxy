http://:8181 {
    route /k8s/* {
        handle_path /k8s/* {
            reverse_proxy * fake {
                transport apiserver {
                    kubeconfig {$KUBECONFIG}
                }
                header_up X-Real-IP {http.request.remote}
                header_up X-Forwarded-Port {http.request.port}
            }
        }
    }
    route * {
		respond "Not Found"
	}
}
